<h1>Produtos</h1>
<div class="products">
    <div class="product">
        {{#each products}}
        <h2>Produto:{{this.name}}</h2>
        <img src="{{this.image}}" alt="{{this.name}}">
        <p>Preço: {{this.price}}</p>
        <p>Descrição: {{this.description}}</p>
        {{/each}}
    </div>
</div>